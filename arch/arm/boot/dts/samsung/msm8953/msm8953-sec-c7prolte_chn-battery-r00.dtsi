	&i2c_13 {
		da9155-charger@58 {
			status = "okay";
			compatible = "dlg,da9155-charger";
			reg = <0x58>;
			interrupt-parent = <&tlmm>;
			interrupts = <48 0>;
			da9155-charger,irq-gpio = <&tlmm 48 0x0>;
			pinctrl-names = "default";
			pinctrl-0 = <&da9155_intr_default>;
		};
